{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "alaouy/youtube": "^2.2",
        "artesaos/seotools": "^0.20.1",
        "cviebrock/eloquent-sluggable": "^8.0",
        "cybercog/laravel-ban": "^4.6",
        "diglactic/laravel-breadcrumbs": "^7.0",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.4",
        "jorenvanhocht/laravel-share": "^4.1",
        "lab404/laravel-impersonate": "^1.7",
        "laravel/framework": "^8.54",
        "laravel/sanctum": "^2.11",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.5",
        "laravelcollective/html": "^6.2",
        "livewire/livewire": "^2.5",
        "renatomarinho/laravel-page-speed": "^2.0",
        "spatie/laravel-analytics": "^3.11",
        "spatie/laravel-backup": "^6.16",
        "spatie/laravel-feed": "^3.2",
        "spatie/laravel-newsletter": "^4.10",
        "spatie/laravel-responsecache": "^6.6",
        "spatie/laravel-sitemap": "^5.9",
        "spatie/schema-org": "^3.4",
        "uxweb/sweet-alert": "^2.0",
        "yoeunes/toastr": "^1.2"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "laravel/ui": "^3.3",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
